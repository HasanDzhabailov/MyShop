<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BlueButton" parent="TextAppearance.AppCompat.Button">
        <item name="android:background">@drawable/button</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>

    </style>

    <style name="ThemeOverlay.Toolbar" parent="ThemeOverlay.MaterialComponents.Toolbar.Primary">
        <item name="android:editTextColor">@color/back_gray_search</item>
        <item name="android:textColorHint">@color/back_gray_search</item>
        <item name="android:textSize">9sp</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="Theme.MyShop.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorOnPrimary">@color/black</item>
    </style>

    <style name="Theme.MyShop.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorOnPrimary">@color/black</item>
    </style>
</resources>